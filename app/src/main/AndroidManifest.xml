<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="youngjee.com.examapp01" >

    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ArrayListActivity"
            android:label="@string/title_activity_array_list" >
        </activity>
        <activity
            android:name=".UserAdapterActivity"
            android:label="@string/title_activity_user_adapter" >
        </activity>
        <activity
            android:name=".ArrayList2Activity"
            android:label="@string/title_activity_array_list2" >
        </activity>
        <activity
            android:name=".AssertImageReadActivity"
            android:label="@string/title_activity_assert_image_read" >
        </activity>
        <activity
            android:name=".MemoryCheckActivity"
            android:label="@string/title_activity_memory_check" >
        </activity>
        <activity
            android:name=".ActionBarSampleActivity"
            android:label="@string/title_activity_action_bar"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" >
        </activity>
        <activity
            android:name=".DataSenderActivity"
            android:label="@string/title_activity_data_sender" >
        </activity>
        <activity
            android:name=".DataReceiverActivity"
            android:label="@string/title_activity_data_receiver" >
        </activity>
        <activity
            android:name=".TakePictureActivity"
            android:label="@string/title_activity_take_picture" >
        </activity>
        <activity
            android:name=".TouchEventExamActivity"
            android:label="@string/title_activity_touch_event_exam" >
        </activity>
        <activity
            android:name=".TapHostActivity"
            android:label="@string/title_activity_tap_host" >
        </activity>
        <activity
            android:name=".MakeDialogActivity"
            android:label="@string/title_activity_make_dialog" >
        </activity>
        <activity
            android:name=".MakeProgressActivity"
            android:label="@string/title_activity_make_progress" >
        </activity>
        <activity
            android:name=".WakeLockActivity"
            android:label="@string/title_activity_wake_lock" >
        </activity>
        <activity
            android:name=".DynaMicScrollActivity"
            android:label="@string/title_activity_dyna_mic_scroll" >
        </activity>
        <activity
            android:name=".UILayoutActivity"
            android:label="@string/title_activity_uilayout"
            android:parentActivityName=".MainActivity" >

            <!-- parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".SharedPreferencesActivity"
            android:label="@string/title_activity_shared_preferences" >
        </activity>
        <activity
            android:name=".FileWriteActivity"
            android:label="@string/title_activity_file_write" >
        </activity>
        <activity
            android:name=".SqlHandleActivity"
            android:label="@string/title_activity_sql_handle" >
        </activity>
        <activity
            android:name=".ImplicitActivity"
            android:label="@string/title_activity_implicit" >
        </activity>
        <activity
            android:name=".DialogCallBackActivity"
            android:label="@string/title_activity_dialog_call_back" >
        </activity>
        <activity
            android:name=".TestNotificationActivity"
            android:label="@string/title_activity_test_notification" >
        </activity>
        <activity
            android:name=".AutoCompleteTextActivity"
            android:label="@string/title_activity_auto_complete_text" >
        </activity>
        <activity
            android:name=".ViewPagerMainActivity"
            android:label="@string/title_activity_view_pager_main" >
        </activity>
        <activity
            android:name=".TextWatcherActivity"
            android:label="@string/title_activity_text_watcher" >
        </activity>
    </application>

</manifest>
