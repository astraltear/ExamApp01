<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="youngjee.com.examapp01.WakeLockActivity">


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="WakeLock"
        android:id="@+id/btn_setOn"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="34dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textView3"
        android:layout_below="@+id/btn_setOn"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="51dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="vibate"
        android:id="@+id/btn_vibrate1"
        android:layout_below="@+id/textView3"
        android:layout_alignLeft="@+id/btn_setOn"
        android:layout_alignStart="@+id/btn_setOn"
        android:layout_marginTop="26dp" />


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="vibate2"
        android:id="@+id/btn_vibrate2"
        android:layout_below="@+id/btn_vibrate1"
        android:layout_alignLeft="@+id/btn_setOn"
        android:layout_alignStart="@+id/btn_setOn"
        android:layout_marginTop="26dp" />


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="vibateCancle"
        android:id="@+id/btn_vibrateCancle"
        android:layout_below="@+id/btn_vibrate2"
        android:layout_alignLeft="@+id/btn_setOn"
        android:layout_alignStart="@+id/btn_setOn"
        android:layout_marginTop="26dp" />
</RelativeLayout>
