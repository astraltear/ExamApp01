<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="youngjee.com.examapp01.ImplicitActivity">


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="calling"
        android:id="@+id/btn_calling"
        android:onClick="goCalling"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="viewMap"
        android:id="@+id/btn_viewMap"
        android:onClick="goViewMap"
        android:layout_below="@+id/btn_calling"
        android:layout_centerHorizontal="true" />


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="viewWeb"
        android:id="@+id/btn_viewWeb"
        android:onClick="goViewWeb"
        android:layout_below="@+id/btn_viewMap"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
